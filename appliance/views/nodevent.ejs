<%- 
  deps
%>

NoDevent.ready = function(fn) {
  if(NoDevent.is_ready) {
    return fn();
  }
  head.js(     
    "<%- opts.host + "/socket.io/socket.io.js" %>",
    function() {
      var connect_url = "<%- opts.host + "/" + opts.namespace %>";
      NoDevent.socket = io.connect(connect_url );
      NoDevent._ready(fn);
    });
};



      